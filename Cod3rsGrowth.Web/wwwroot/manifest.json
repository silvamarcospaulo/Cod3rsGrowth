{
	"_version": "1.58.0",
	"sap.app": {
	  "id": "ui5.mtgdeckbuider",
	  "i18n": "i18n/i18n.properties",
	  "type": "application",
	  "applicationVersion": {
		"version": "1.0.0"
	  }
	},
	"sap.ui": {
	  "technology": "UI5",
	  "deviceTypes": {
		"desktop": true
	  }
	},
	"sap.ui5": {
	  "dependencies": {
		"minUI5Version": "1.108.0",
		"libs": {
		  "sap.ui.core": {},
		  "sap.m": {}
		}
	  },
	  "models": {
		"i18n": {
		  "type": "sap.ui.model.resource.ResourceModel",
		  "settings": {
			"bundleName": "ui5.mtgdeckbuilder.i18n.i18n",
			"supportedLocales": ["pt", "en"],
			"fallbackLocale": "pt"
			
		  }
		}
	  },
	  "rootView": {
		"viewName": "ui5.mtgdeckbuilder.webapp.App",
		"type": "XML",
		"id": "App"
	  },
  
	  "routing": {
		"config": {
		  "routerClass": "sap.m.routing.Router",
		  "type": "View",
		  "viewType": "XML",
		  "path": "ui5.mtgdeckbuilder.webapp",
		  "controlId": "app",
		  "controlAggregation": "pages",
		  "async": true
		},
		"routes": [
		  {
			"pattern": "",
			"name": "app",
			"target": "app"
		  }
		],
		"targets": {
		  "app": {
			"id": "app",
			"name": "App"
		  }
		}
	  }
	}
  }