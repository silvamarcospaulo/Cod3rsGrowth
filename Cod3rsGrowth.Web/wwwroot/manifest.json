{
	"_version": "1.58.0",
	"sap.app": {
		"id": "mtgdeckbuider",
		"i18n": "i18n/i18n.properties",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"mobile": true
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.108.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "mtgdeckbuilder.i18n.i18n",
					"fallbackLocale": ""
				}
			}
		},
		"rootView": {
			"viewName": "mtgdeckbuilder.app.App",
			"type": "XML",
			"id": "App"
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "mtgdeckbuilder.app",
				"controlId": "app",
				"controlAggregation": "pages",
            	"transition": "slide",
				"bypassed": {
					"target": "notFound"
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "listagemJogador",
					"target": "listagemJogador"
				},
				{
					"pattern": "criacaoJogador",
					"name": "criacaoJogador",
					"target": "criacaoJogador"
				},
				{
					"pattern": "detalhesJogador/{id}",
					"name": "detalhesJogador",
					"target": "detalhesJogador"
				},
				{
					"pattern": "edicaoJogador/{id}",
					"name": "edicaoJogador",
					"target": "edicaoJogador"
				}
			],
			"targets": {
				"notFound": {
					"id": "notFound",
					"path": "mtgdeckbuilder.app.notFound",
					"name": "NotFound"
				},
				"listagemJogador": {
					"id": "listagemJogador",
					"path": "mtgdeckbuilder.app.jogador",
					"name": "ListagemJogador"
				},
				"criacaoJogador": {
					"id": "criacaoJogador",
					"path": "mtgdeckbuilder.app.jogador",
					"name": "CriacaoJogador"
				},
				"detalhesJogador": {
					"id": "detalhesJogador",
					"path": "mtgdeckbuilder.app.jogador",
					"name": "DetalhesJogador"
				},
				"edicaoJogador": {
					"id": "edicaoJogador",
					"path": "mtgdeckbuilder.app.jogador",
					"name": "EdicaoJogador"
				}
			}
		}
	}
}