<mvc:View
    controllerName="mtgdeckbuilder.app.jogador.CriacaoJogador"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns="sap.m">

    <Page
        title="{i18n>App}"
        showNavButton="true"
        navButtonPress=".aoPressionarRetornarNavegacao">
        <VBox class="sapUiSmallMargin">
            <f:Form
                editable="false">
                <f:toolbar>
                    <Toolbar>
                        <Title id="Title1" text="{i18n>CriacaoJogador.Toolbar.Titulo}" />
                    </Toolbar>
                </f:toolbar>
                <f:layout>
                    <f:ResponsiveGridLayout
                        labelSpanXL="4"
                        labelSpanL="3"
                        labelSpanM="4"
                        labelSpanS="1"
                        adjustLabelSpan="true"
                        emptySpanXL="0"
                        emptySpanL="4"
                        emptySpanM="0"
                        emptySpanS="0"
                        columnsXL="2"
                        columnsL="1"
                        columnsM="1"
                        singleContainerFullSize="true" />
                </f:layout>
                <f:formContainers>



                </f:formContainers>
            </f:Form>
        </VBox>
    </Page>
</mvc:View>